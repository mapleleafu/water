generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Subscription {
  id        String   @id @default(uuid())
  endpoint  String   @unique // The URL identifying the user's browser
  keys      Json // Stores auth and p256dh keys
  timezone  String   @default("UTC")
  createdAt DateTime @default(now())
}

model DrinkLog {
  id        Int      @id @default(autoincrement())
  timestamp DateTime @default(now())
}
